{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon/(.*)",
      "dest": "/static/favicon/$1"
    },
    {
      "src": "/video/(.*)",
      "dest": "/static/video/$1"
    },
    {
      "src": "/videos/(.*)",
      "dest": "/static/videos/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/static/img/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/static/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/static/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "FLASK_APP": "app.py",
    "FLASK_ENV": "production",
    "SESSION_SECRET": "freire-fpv-production-secret"
  }
}