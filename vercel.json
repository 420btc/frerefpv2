{
  "version": 2,
  "functions": {
    "app.py": {
      "includeFiles": "static/**"
    }
  },
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "FLASK_APP": "app.py",
    "FLASK_ENV": "production",
    "SESSION_SECRET": "freire-fpv-production-secret"
  }
}
